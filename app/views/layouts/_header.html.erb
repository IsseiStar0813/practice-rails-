<header>
  <div class = "container">
    <%= link_to "Rails practice", root_path , id:"logo"%>
    <nav>
      <ul class = "linkList">
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "About", about_path %></li>
        <% if current_user %>
        <li><%= link_to "Mypage", user_path(current_user) %></li>
        <li><%= link_to "Log out", "#" %></li>
        <% else %>
        <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>